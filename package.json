{
  "name": "vds-monorepo",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "sass --watch --load-path=node_modules --load-path=packages apps/showroom/showroom.scss:apps/showroom/showroom.css",
    "dev:landing": "sass --watch --load-path=node_modules --load-path=packages apps/landing-page/landing.scss:apps/landing-page/landing.css"
  },
  "dependencies": {
    "lottie-web": "^5.13.0"
  }
}